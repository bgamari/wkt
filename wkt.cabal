category:            Data
build-type:          Simple
cabal-version:       >=1.10

library
  exposed-modules:     Data.Geo.WKT, Data.Geo.WKT.Types, Data.Geo.WKT.Parser
  other-extensions:    TemplateHaskell
  build-depends:       base >=4.6 && <4.7,
                       lens >=3.9 && <3.10,
                       linear >=1.3 && <1.4,
                       parsec >=3.1 && <3.2,
                       parsec-numbers >=0.0 && <0.1
  hs-source-dirs:      src
  default-language:    Haskell2010
